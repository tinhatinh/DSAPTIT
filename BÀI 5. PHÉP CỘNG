#include <iostream>
#include <string>

using namespace std;

void solve() {
    string s;
    cin >> s;
    int a = s[0] - '0';
    int b = s[4] - '0';
    int c = s[8] - '0';
    if (a + b == c) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }
}

int main() {
    solve();
    return 0;
}
